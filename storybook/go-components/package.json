{
    "name": "@goaigua/go-components",
    "version": "3.2.0",
    "author": "Go Aigua - Idrica",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "module": "dist/go-components.esm.js",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "storybook": "start-storybook -p 9009",
        "docs": "npx styleguidist server",
        "clean": "rimraf dist/",
        "build-storybook": "build-storybook -c .storybook -o public",
        "compile": "npm run clean && tsdx build",
        "npmupload:patch": "npm version patch && npm run compile && npm publish --registry https://nexus.dev.idrica.pro/repository/npm-hosted/",
        "npmupload:minor": "npm version minor && npm run compile && npm publish --registry https://nexus.dev.idrica.pro/repository/npm-hosted/",
        "npmupload:major": "npm version major && npm run compile && npm publish --registry https://nexus.dev.idrica.pro/repository/npm-hosted/"
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "publishConfig": {
        "registry": "https://nexus.dev.idrica.pro/repository/npm-hosted"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint -- --fix"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": false,
        "tabWidth": 4,
        "endOfLine": "auto",
        "trailingComma": "es5"
    },
    "size-limit": [
        {
            "path": "dist/go-components.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/go-components.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/helper-validator-identifier": "^7.10.4",
        "@mdx-js/loader": "^1.6.22",
        "@size-limit/preset-small-lib": "^4.9.1",
        "@storybook/addon-actions": "^6.0.26",
        "@storybook/addon-controls": "^6.1.11",
        "@storybook/addon-docs": "^6.1.21",
        "@storybook/addon-essentials": "^6.1.21",
        "@storybook/addon-info": "^5.3.21",
        "@storybook/addon-knobs": "^6.0.26",
        "@storybook/addon-links": "^6.1.10",
        "@storybook/addons": "^6.1.10",
        "@storybook/preset-create-react-app": "^3.1.5",
        "@storybook/react": "^6.1.21",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^13.1.9",
        "@types/jest": "^26.0.23",
        "@types/node": "^14.14.13",
        "@types/react": "^17.0.0",
        "@types/react-dates": "^21.8.1",
        "@types/react-dom": "^17.0.0",
        "babel-loader": "^8.2.2",
        "css-loader": "^3.6.0",
        "husky": "^4.3.5",
        "identity-obj-proxy": "^3.0.0",
        "postcss": "^8.2.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "@types/testing-library__jest-dom": "^5.13.0",
        "react-is": "^17.0.1",
        "react-moment-proptypes": "^1.7.0",
        "react-scripts": "^3.4.4",
        "react-styleguidist": "^11.1.5",
        "rimraf": "^3.0.2",
        "rollup-plugin-postcss": "^4.0.0",
        "sass": "^1.30.0",
        "sass-loader": "^8.0.2",
        "size-limit": "^4.9.1",
        "storybook-dark-mode": "1.0.3",
        "style-loader": "1.2.1",
        "tsdx": "^0.14.1",
        "tslib": "^2.0.3",
        "typescript": "^4.2.2"
    },
    "dependencies": {
        "highcharts": "^9.0.1",
        "highcharts-react-official": "^3.0.0",
        "moment": "^2.29.1",
        "react-dates": "^21.8.0"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less|scss|sass)$": "identity-obj-proxy"
        }
    }
}
